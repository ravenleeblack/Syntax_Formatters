{
	"name": "seedling-language",
	"displayName": "Seedling",
	"description": "Language support for Seedling files",
	"version": "0.0.1",
	"publisher": "Raven Black",
	"engines": {
		"vscode": "^1.75.0"
	},
	"categories": [
		"Programming Languages"
	],
